import os
import re

def get_latest_version(directory, prefix):
    pattern = re.compile(r'^' + re.escape(prefix) + r'-(\d+\.\d+)\.jar$')
    latest_version = None
    latest_file = None

    for filename in os.listdir(directory):
        match = pattern.match(filename)
        if match:
            version = tuple(map(int, match.group(1).split('.')))  # Convert version to tuple (e.g., (2, 32))
            if latest_version is None or version > latest_version:
                latest_version = version
                latest_file = filename

    if latest_file:
        return os.path.realpath(os.path.join(directory, latest_file))
    return None

# Example usage
directory = "/path/to/jar/files"
prefix = "fx-extraction"
latest_file_path = get_latest_version(directory, prefix)

if latest_file_path:
    print("Latest file:", latest_file_path)
else:
    print("No matching files found.")