import org.apache.spark.SparkConf;
import org.apache.spark.api.java.JavaSparkContext;

public class SparkAppNameExample {
    public static void main(String[] args) {
        // Initialize SparkConf and JavaSparkContext
        SparkConf sparkConf = new SparkConf().setAppName("DefaultAppName"); // Optional fallback name
        JavaSparkContext sparkContext = new JavaSparkContext(sparkConf);

        // Get the application name
        String appName = sparkContext.getConf().get("spark.app.name");

        // Print the application name
        System.out.println("Application Name: " + appName);

        // Stop the context
        sparkContext.stop();
    }
}